    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="ChooseDebugConfiguration">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="ctrl r" />
  </action>
  <action id="ChooseRunConfiguration">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="r" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="alt semicolon" />
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="semicolon" />
  </action>
  <action id="Debug" />
  <action id="EditorPasteFromX11" />
  <action id="EmmetNextEditPoint" />
  <action id="EmmetPreviousEditPoint" />
