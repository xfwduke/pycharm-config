    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="Generate">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="i" />
  </action>
  <action id="NewElement">
    <keyboard-shortcut first-keystroke="ctrl c" second-keystroke="n" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="shift alt close_bracket" />
    <mouse-shortcut keystroke="button5" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="shift alt open_bracket" />
    <mouse-shortcut keystroke="button4" />
  </action>
